CONFIG_CC_IS_GCC=n
CONFIG_CC_HAS_ASM_GOTO=n
CONFIG_CC_CAN_LINK=y
# ativai boot sem GUB
CONFIG_EFI_STUB=y
# indica que o compilador (GCC) possui suporte para um aviso específico 
CONFIG_CC_HAS_WARN_MAYBE_UNINITIALIZED=n
# configuração indica que os avisos sobre variáveis 
CONFIG_CC_DISABLE_WARN_MAYBE_UNINITIALIZED=y
# Permite que o kernel lide rapidamente com interrupções 
CONFIG_IRQ_WORK=y
